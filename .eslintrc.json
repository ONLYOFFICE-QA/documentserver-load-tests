{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "max-len": ["error", { "code": 100 }],
      "camelcase": [2, {"properties": "always", "ignoreImports": false}],
      "new-cap": ["error", { "capIsNew": false }]
    }
}
